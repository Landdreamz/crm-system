# Copy this file to .env and fill in your Twilio values.
# Get them from https://console.twilio.com (see docs/TWILIO.md).

# Required for Power Dialer
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_API_KEY_SID=
TWILIO_API_KEY_SECRET=
TWILIO_TWIML_APP_SID=

# Optional: caller ID for outbound calls (E.164, e.g. +15551234567)
TWILIO_NUMBER=

# GoHighLevel (sync contacts to CRM)
GOHIGHLEVEL_API_KEY=
GOHIGHLEVEL_LOCATION_ID=
# APN custom field id (required for APN import; get from GHL or debug output ?debug=1)
GOHIGHLEVEL_APN_FIELD_ID=
# Lot size / sqft custom field id
GOHIGHLEVEL_LOT_SIZE_FIELD_ID=
# Acres / acreage custom field id (GHL: contact.acreage)
GOHIGHLEVEL_ACRES_FIELD_ID=
# Latitude / longitude (GHL: contact.latitude, contact.longitude)
GOHIGHLEVEL_LATITUDE_FIELD_ID=
GOHIGHLEVEL_LONGITUDE_FIELD_ID=
